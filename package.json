{
  "name": "mern-devops",
  "version": "1.0.0",
  "description": "Basic setup for mern with travis CI",
  "homepage": "https://rmsingh83.github.io/MERN_DEVOPS/",
  "main": "bootstrap.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-serve --open --port 3000",
    "error": "webpack --display-error-details",
    "build": "cross-env NODE_ENV=production webpack",
    "prebuild": "mkdirp build && npm run clean && cross-env CI=true npm t",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "cypress": "cypress open",
    "e2e": "cypress run",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:headed": "cypress run --headed",
    "test": "jest --bail --colors --verbose",
    "test:watch": "jest --watchAll",
    "doc": "jest --coverage",
    "clean": "rimraf build",
    "clean:doc": "rimraf /coverage/*",
    "precommit": "lint-staged",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx}": [
        "eslint --fix",
        "jest --bail --findRelatedTests",
        "git add"
      ],
      "**/*.scss": [
        "stylelint --syntax=scss",
        "git add"
      ]
    }
  },
  "jest": {
    "automock": false,
    "clearMocks": true,
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/coverage/",
      "<rootDir>/e2e/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "author": "Ram Singh <ramsingh_83@yahoo.com>",
  "license": "NOLICENSE",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "lodash": "4.17.11",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-hot-loader": "4.6.5",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "^3.1.3",
    "browserslist": "4.4.1",
    "clean-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "cypress": "^3.1.5",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "eslint": "5.12.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.2.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
